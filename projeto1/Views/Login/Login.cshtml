@model projeto1.Models.Usuario;
@{
    ViewData["Title"] = "Login";

}
<h1>Login</h1>
<form asp-action="Login" method="post">
    <div>
        <label>E-mail</label>
        <input type="email" name="email" required/>
       
    </div>
    <div>
        <label>Senha</label>
        <input type="password" name="Senha" required />
    </div>
    <buttom type="submit">Login</buttom>
</form>
